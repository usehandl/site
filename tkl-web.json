{
  "family": "tkl-web",
  "executionRoleArn": "arn:aws:iam::097047758552:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "tkl-web",
      "image": "097047758552.dkr.ecr.ap-south-1.amazonaws.com/tkl-web",
      "portMappings": [
        {
          "containerPort": 3001,
          "hostPort": 3001,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/tkl-web",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "secrets": [
        {
          "name": "NODE_ENV",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:097047758552:secret:tkl-uh9aOl:NODE_ENV::"
        }
      ] 
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "4096"
}
